<div class="page-header">
  <h1>City's <small>[<%= link_to 'New', new_admin_city_path %>]</small></h1>
</div>

<table class="table table-striped">
  <thead>
    <th>#</th>
    <th>Name</th>
    <th>Slug</th>
    <th>Actions</th>
  </thead>
  <tbody>
    <% @cities.each do |city| %>
    <tr>
      <td><%= city.id %></td>
      <td><%= city.name %></td>
      <td><%= city.slug %></td>
      <td><%= link_to '[edit]', edit_admin_city_path(city) %> <%= link_to '[delete]', admin_city_path(city), method: 'delete' %></td>
    </tr>
    <% end %>
  </tbody>
</table>
